{
    "manifestVersion": 1,
    "id": "ivamuno-azure-boards",
    "version": "1.0.0",
    "name": "[Ivamuno] Azure Boards",
    "description": "Set of dashboard widgets to monitor Ivamuno work flow.",
    "publisher": "Ivamuno",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.work",
        "vso.wiki",
        "vso.wiki_write"
    ],
    "public": false,
    "icons": {
        "default": "img/icon.png"
    },
    "demands": [
        "contribution/ms.vss-dashboards-web.widget-sdk-version-2"
    ],
    "contributions": [
        {
            "id": "WidgetBuilder",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                ".WidgetBuilderConfiguration"
            ],
            "properties": {
                "name": "Widget to build other builders (v2).",
                "description": "Widget with a text field to insert the code and test (v2).",
                "catalogIconUrl": "widgets/WidgetBuilder/img/builder.png",
                "previewImageUrl": "widgets/WidgetBuilder/img/builder_330X160.png",
                "uri": "widgets/WidgetBuilder/WidgetBuilder.html",
                "isNameConfigurable": true,
                "supportedSizes": [
                    {
                        "rowSpan": 1,
                        "columnSpan": 1
                    },
                    {
                        "rowSpan": 1,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 7
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 8
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 9
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 7
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 8
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 9
                    },
                    {
                        "rowSpan": 5,
                        "columnSpan": 10
                    }
                ],
                "lightbox": {
                    "resizable": true,
                    "width": 900,
                    "height": 700
                },
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "WidgetBuilderConfiguration",
            "type": "ms.vss-dashboards-web.widget-configuration",
            "targets": [
                "ms.vss-dashboards-web.widget-configuration"
            ],
            "properties": {
                "name": "WidgetBuilder Configuration",
                "description": "Configures WidgetBuilder",
                "uri": "widgets/WidgetBuilder/WidgetBuilderConfig.html"
            }
        }
    ],
    "files": [
        {
            "path": "widgets/WidgetBuilder/WidgetBuilder.html",
            "addressable": true
        },
        {
            "path": "widgets/WidgetBuilder/WidgetBuilderConfig.html",
            "addressable": true
        },
        {
            "path": "widgets/WidgetBuilder/lib",
            "addressable": true
        },
        {
            "path": "widgets/WidgetBuilder/img",
            "addressable": true
        },
        {
            "path": "widgets/WidgetBuilder/css",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}